<string-expression> ::= '"' string '"' ';' | ''' string ''' ';'

<bool-expression> ::= 'false' ';' | 'true' ';' | <comparison> ';' 

<comparison> ::= integer '==' integer ';' | integer '!=' integer ';' | integer '<=' integer ';' | integer '=>' integer ';'



