
<blocks> ::= <block>,<blocks> | <block>

<block> ::= <assignment> | <output> | <input> | <condition> | <loop>


<declartion> :: = <datatype> ' ' <variable> '=' <expression> ';'
| <datatype> ' ' <variable> ';'

<assignment> ::= <variable> '=' <expression> ';'

<output> ::= 'print' '(' <expression> ')' ';' | 'print' '(' <variable> ')' ';'

<input> ::= 'get_input' '(' <string-expression> ')' ';'

<if-condition> ::= 'if' '(' <bool-expression> ')' '{' <blocks> '}' ';' 
| 'if' '(' <bool-expression> ')' '{' <blocks> '}' ';' '<elseif> 
| 'if' '(' <bool-expression> ')' '{' <blocks> '}' ';' '<elseif> 'else' '{' <blocks> '}' ';'


<function> ::= 'def' <datatype> <string-expresison>'(' <parameters> ')' '{' <blocks> '}' ';'

<parameters> ::= <datatype> ' ' <variable>
| <datatype> ' ' <variable> ' ' <parameters>


<elseif> ::= 'elseif' '(' <bool-expression> ')' '{' <blocks> '}' ';'
| 'elseif' '(' <bool-expression> ')' ';' '{' <blocks> '}' <elseif> ';'


<loop> ::= 'for' '(' <assignment> ';' <condition> ';' <atrim-expression> ')' '{' <blocks> '}' ';'				!! <iterable> ::= <list> // Hur gör vi med iterable?
| 'for_each' '(' <iterable> ')' '{' <blocks> '}' ';'
| 'while' '(' <condition> ')' '{' <blocks> '}' ';'
 

<expression> ::= <artim-expression> | <string-expresison> | <bool-expression>


#<artim-expression> ::= <term> '+' <artim-expression> ';'
#| <term> ';'
#
#<term> ::= <factor> '*' <term>
#| <factor>
#
#<factor> ::= '(' <artim-expression> ')'
#| <const>
#
#<const> ::= integer
#

<string-expression> ::= '"' string '"' ';' | ''' string ''' ';'

<bool-expression> ::= 'false' ';' | 'true' ';' | <comparison> ';' 

<comparison> ::= integer '==' integer ';' | integer '!=' integer ';' | integer '<=' integer ';' | integer '=>' integer ';'


